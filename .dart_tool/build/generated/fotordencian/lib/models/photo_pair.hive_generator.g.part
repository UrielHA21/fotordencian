class PhotoPairAdapter extends TypeAdapter<PhotoPair> {
  @override
  final int typeId = 1;

  @override
  PhotoPair read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return PhotoPair(
      beforeImagePath: fields[0] as String,
      afterImagePath: fields[1] as String,
    );
  }

  @override
  void write(BinaryWriter writer, PhotoPair obj) {
    writer
      ..writeByte(2)
      ..writeByte(0)
      ..write(obj.beforeImagePath)
      ..writeByte(1)
      ..write(obj.afterImagePath);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is PhotoPairAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
